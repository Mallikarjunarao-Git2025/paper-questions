{
  "name": "paper-questions",
  "version": "0.0.1",
  "description": "Paper Question Sheets for Math and Music.",
  "author": "Kyle Geske",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/stungeye/paper-questions"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "gh-pages": "^2.0.1",
    "now": "^13.0.0",
    "parcel-bundler": "^1.11.0",
    "parcel-plugin-clean-dist": "0.0.6",
    "surge": "^0.20.1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": [
            {
              "node": "current"
            }
          ]
        }
      ]
    ],
    "plugins": [
      [
        "babel-plugin-transform-builtin-extend",
        {
          "globals": [
            "Error"
          ]
        }
      ],
      [
        "transform-regenerator"
      ]
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "lint-test": "eslint . && jest --no-cache ./* ",
    "dev": "parcel index.html",
    "build": "parcel build index.html",
    "heroku-postbuild": "parcel build index.html",
    "surge": "surge",
    "build-gh-pages": "parcel build index.html --public-url /paper-questions",
    "deploy-gh-pages": "gh-pages -d dist",
    "now": "now",
    "now-from-dist": "now --name musical-bears deploy ./dist",
    "now-alias": "now alias https://musical-bears.now.sh",
    "now-build": "parcel build index.html"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "node": true
    },
    "extends": "airbnb-base",
    "rules": {
      "import/no-unresolved": "off",
      "import/extensions": "off",
      "import/prefer-default-export": "off",
      "import/no-default-export": "off"
    }
  },
  "license": "UNLICENSE",
  "dependencies": {
    "abcjs": "^5.6.1"
  }
}
